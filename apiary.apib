    curl -H "Authorization: Bearer 456b4d05baacbd232efbefc5b08c2be7c5de40ee" https://staging.gworld.in.th/api/v1/groups/32/feed -X POST -F body=descriptionasdf -F privacy_post=1 -F attach_file=@gmm.jpg
    
